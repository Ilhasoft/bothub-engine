[run]
source =
    ./bothub/api
    ./bothub/authentication
    ./bothub/common
omit =
    ./bothub/api/apps.py
    ./bothub/authentication/apps.py
    ./bothub/common/apps.py
    ./bothub/api/v2/swagger.py
    ./bothub/api/v2/tests/*
    ./bothub/api/v2/nlp/*
    ./bothub/authentication/tests.py
    ./bothub/common/tests.py
    ./bothub/common/management/*
    ./bothub/common/migrations/*

[report]
fail_under = 90